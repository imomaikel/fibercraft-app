{
  "name": "fibercraft-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "dev:bot": "cross-env NODE_ENV=development BOT_ONLY=true nodemon --exec ts-node bot/client.ts",
    "build": "cross-env NODE_ENV=production npm run build:server && npm run build:next",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "cross-env NEXT_BUILD=true node dist/server/index.js",
    "lint": "eslint ./app && eslint ./bot && eslint ./server",
    "production": "npm install && npx prisma generate && npm run build && npm run start",
    "prepare": "husky"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "@trpc/client": "^10.44.1",
    "@trpc/react-query": "^10.44.1",
    "@trpc/server": "^10.44.1",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mysql": "^2.18.1",
    "next": "14.0.3",
    "react": "^18",
    "react-dom": "^18",
    "superjson": "1.13.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@types/express": "^4.17.21",
    "@types/mysql": "^2.15.25",
    "@types/node": "^20.10.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-config-next": "14.0.3",
    "husky": "^9.0.11",
    "postcss": "^8",
    "prisma": "^5.9.1",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
