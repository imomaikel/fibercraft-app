{
  "name": "fibercraft-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "dev:bot": "cross-env NODE_ENV=development BOT_ONLY=true nodemon --exec ts-node bot/client.ts",
    "build": "cross-env NODE_ENV=production npm run build:server && npm run build:next",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "cross-env NEXT_BUILD=true node dist/server/index.js",
    "lint": "eslint ./app && eslint ./bot && eslint ./server",
    "production": "npm install && npx prisma generate && npm run build && npm run start",
    "prepare": "husky"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.10.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@trpc/client": "^10.44.1",
    "@trpc/react-query": "^10.44.1",
    "@trpc/server": "^10.44.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.1",
    "date-fns": "^3.3.1",
    "discord.js": "^14.12.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mysql": "^2.18.1",
    "next": "14.0.3",
    "next-auth": "^4.24.6",
    "rcon-client": "^4.2.4",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.0.1",
    "sonner": "^1.4.3",
    "superjson": "1.13.3",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "usehooks-ts": "^2.15.1",
    "zod": "^3.22.4",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@types/express": "^4.17.21",
    "@types/mysql": "^2.15.25",
    "@types/node": "^20.10.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-config-next": "14.0.3",
    "husky": "^9.0.11",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "prisma": "^5.10.2",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
