{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@ui/*": ["./app/(assets)/components/ui/*"],
      "@trpc/*": ["./app/(assets)/trpc/*"],
      "@/*": ["./app/(dashboard)/*"],
      "@shop/*": ["./app/(shop)/shop/*"],
      "@assets/*": ["./app/(assets)/*"],
      "@management/*": ["./app/(management)/components/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
